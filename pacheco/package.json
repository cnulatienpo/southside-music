{
  "name": "pacheco-environment",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/main/electron-main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "npm-run-all -p dev:renderer dev:main dev:electron",
    "dev:renderer": "vite",
    "dev:main": "tsc -w --project tsconfig.main.json",
    "dev:electron": "wait-on dist/main/main/electron-main.js && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron dist/main/main/electron-main.js",
    "build": "npm-run-all build:renderer build:main",
    "build:renderer": "vite build",
    "build:main": "tsc --project tsconfig.main.json",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "electron": "^29.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.8",
    "wait-on": "^7.2.0"
  }
}
