{
  "name": "southside-music",
  "version": "0.1.0",
  "private": true,
  "main": "main/main.js",
  "scripts": {
    "start": "concurrently \"npm run react-start\" \"npm run electron-start\"",
    "dev": "npm run start",
    "react-start": "react-scripts start",
    "electron-start": "wait-on http://localhost:3000 && electron ./main/main.js",
    "build": "react-scripts build && electron-builder"
  },
  "dependencies": {
    "nanoid": "^5.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-reload": "^2.0.0",
    "ts-node": "^10.9.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  }
}
